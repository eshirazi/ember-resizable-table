{{! template-lint-disable no-action }}
<ResizableTable
  @class='main-table'
  @onSizeChanged={{action this.eventTriggered 'on-size-changed'}}
  as |table|
>

  <table.tr as |row|>
    {{! template-lint-disable no-curly-component-invocation }}
    {{row.td class='col1'}}
    {{row.td class='col2'}}
  </table.tr>
</ResizableTable>